        {
            "name": "Window 1",
            "widgets": [
                {
                    "type": "chart",
                    "name": "GPS Latitude on secondary Window",
        
                    "source": "anirniq.acquisition.gps.lat",
                    "length": 2000,
        
                    "refresh_rate": 60,
                    "range": [0, 2],
                    "x": 0,
                    "y": 0,
                    "rowspan": 2,
                    "height": 400
                }
            ]
        }
    ]
