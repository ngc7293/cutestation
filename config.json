{
    "series": [
        {
            "source": "anirniq.aquisition.gps.lat",
            "length": 2000
        }
    ],
    "widgets": [
        {
            "type": "chart",
            "source": "anirniq.acquisition.gps.lat",
            "length": 2000,
            "refresh_rate": 60,
            "range": [0, 2],
            "x": 0,
            "y": 0,
            "rowspan": 2
        },
        {
            "type": "button",
            "command": "anirniq.remote_control.engine.sequence.start",
            "source": "anirniq.remote_control.engine.sequence.start",
            "label": "Start",
            "length": 1,
            "refresh_rate": 1,
            "x": 0,
            "y": 1
        },
        {
            "type": "button",
            "command": "anirniq.remote_control.engine.sequence.stop",
            "source": "anirniq.remote_control.engine.sequence.stop",
            "label": "Stop",
            "length": 1,
            "refresh_rate": 1,
            "x": 1,
            "y": 1
        }
    ]
}
